{
  "name": "vue-teddy-store",
  "version": "0.1.21",
  "main": "lib/vue-teddy-store.umd.js",
  "module": "lib/vue-teddy-store.esm.js",
  "unpkg": "lib/vue-teddy-store.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gahabeen/vue-teddy-store"
  },
  "author": "Gabin Desserprit <desserprit.gabin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve ./examples",
    "test:unit:watch": "vue-cli-service test:unit --watch",
    "test:unit": "vue-cli-service test:unit",
    "build": "npm run build:esm & npm run build:umd & npm run build:iife",
    "build:esm": "rollup --config build/rollup.config.js --format es --file lib/vue-teddy-store.esm.js",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file lib/vue-teddy-store.umd.js",
    "build:iife": "rollup --config build/rollup.iife.config.js --format iife --file lib/vue-teddy-store.min.js",
    "prepublish": "npm run build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vue/composition-api": "^1.0.0-beta.4",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "flush-promises": "^1.0.2",
    "rollup": "^2.18.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-internal": "^1.0.4",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^5.1.9",
    "vue-template-compiler": "^2.6.11"
  }
}
